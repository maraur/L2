public class Rse {

	public static boolean allRowSumsEqual(int[][] m){
		int sum = 0; 
		if (m == null)
			return true;
		for (int row=0; row< m.length; row++){
			if(!(sum == rowSum(m[row])) && !(row == 0)){
				return false;
			}
			sum = rowSum(m[row]);
		}
		return true;
	}
	public static int rowSum(int[] v){
		int sum=0;
		if ((v == null) || (v.length == 0)){
			return 0;
		}
		for (int i=0; i < v.length; i++) {
			sum += v[i];
		}
		return sum;
	}
}
